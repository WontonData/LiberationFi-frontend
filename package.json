{
  "name": "LiberationFi-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.liberation.fi/",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.15.5",
    "ethers": "^5.4.4",
    "js-conflux-sdk": "^1.7.0",
    "jsbi": "^3.2.4",
    "less": "^4.1.1",
    "less-loader": "^5.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "web3": "^1.5.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "normalize.css": "^8.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
